[init]
	defaultBranch = master
[user]
	name = Marius van Zundert
	email = marius.vanzundert@gmail.com
[core]
	editor = vim
	pager = delta
[interactive]
	diffFilter = delta --color-only
[color]
	ui = true
[credential]
	helper = osxkeychain
[filter ""]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[commit]
[tag]
	sort = -version:refname
[advice]
	skippedCherryPicks = false
[delta]
	side-by-side = true
	line-numbers = true
[push]
	autoSetupRemote = true
[pull]
	rebase = true
